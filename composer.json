{
    "name": "enricodias/email-validator",
    "type": "library",
    "license": "MIT",
    "description": "Validate and check for disposable/temporary/throw away emails using validator.pizza",
    "keywords": ["email", "disposable", "temporary email", "throw away email", "validator", "validator.pizza"],
    "authors": [
        {
            "name": "Enrico Dias",
            "email": "enrico@enricodias.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "enricodias\\EmailValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "enricodias\\EmailValidator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "phpunit/phpunit": ">5.7 <9"
    }
}
